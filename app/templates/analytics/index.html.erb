            <li><a href="/analytics/index_summary">Index summary</a></li>
            <li><a href="/analytics/table_sizes">Table sizes</a></li>
        </ul>
    </nav>
</aside>

<div class="bandera" id="analytics">
    <table id="table" data-type="current_activity">
        <thead>
            <tr>
                <th>datid</th>
                <th>datname</th>
                <th>pid</th>
                <th>usesysid</th>
                <th>usename</th>
                <th>client_addr</th>
                <th>query</th>
            </tr>
        </thead>
        <tbody id="data">

        </tbody>
    </table>
</div>

<script id="template" type="text/ractive">
    {{#each data}}
        <tr>
            <td>{{datid}}</td>
            <td>{{datname}}</td>
            <td>{{pid}}</td>
            <td>{{usesysid}}</td>
            <td>{{usename}}</td>
            <td>{{client_addr}}</td>
            <td>{{query}}</td>
        </tr>
    {{/each}}
</script>

<script type="text/javascript" src="/js/vendor/require.js"></script>
<script type="text/javascript" src="/js/require_config.js"></script>
